{
  "name": "mcp-manager",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"bun run build:electron:watch\" \"bun run dev:vite\" \"bun run dev:electron\"",
    "build:electron": "bun run build:main && bun run build:preload",
    "build:electron:watch": "concurrently -k \"bun run build:main:watch\" \"bun run build:preload:watch\"",
    "build:main": "esbuild src/main/main.ts --bundle --platform=node --outfile=dist/main.js --external:electron",
    "build:main:watch": "esbuild src/main/main.ts --bundle --platform=node --outfile=dist/main.js --external:electron --watch",
    "build:preload": "esbuild src/main/preload.ts --bundle --platform=node --outfile=dist/preload.js --external:electron",
    "build:preload:watch": "esbuild src/main/preload.ts --bundle --platform=node --outfile=dist/preload.js --external:electron --watch",
    "build:renderer": "vite build",
    "dev:vite": "vite",
    "dev:electron": "wait-on dist/main.js dist/preload.js http://localhost:5173 && electron .",
    "build": "bun run build:electron && bun run build:renderer",
    "start": "electron ."
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "electron": "^28.0.0",
    "highlight.js": "^11.11.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.8",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "wait-on": "^7.2.0"
  }
}
